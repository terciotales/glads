{
    "name": "terciotales/glads",
    "description": "A WordPress plugin for managing ads.",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.9"
    },
    "config": {
        "platform": {
            "php": "7.3"
        },
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "autoload": {
        "psr-4": {
            "Glads\\Includes\\": "includes/",
            "Glads\\Admin\\": "admin/",
            "Glads\\Public\\": "public/"
        }
    }
}
